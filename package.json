{
  "name": "api",
  "version": "1.0.0",
  "description": "API for the fatboar application",
  "main": "server-express.js",
  "scripts": {
    "test": "mocha --timeout 15000",
    "testjunit": "JUNIT_REPORT_PATH=./report.xml mocha --colors --reporter mocha-jenkins-reporter --timeout 15000",
    "start": "nodemon server-express.js",
    "prod": "DOTENV_FLOW_PATH=env nodemon -r dotenv-flow/config server-express.js --node-env=production",
    "dev": "DOTENV_FLOW_PATH=env nodemon -r dotenv-flow/config server-express.js --node-env=development",
    "docker-build": "docker-compose -f docker-compose.yml build",
    "docker-up": "docker-compose -f docker-compose.yml up -d",
    "docker-down": "docker-compose -f docker-compose.yml down",
    "debug": "nodemon --max_old_space_size=7048 --inspect server-express.js"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "async": "^3.1.0",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.1.0",
    "expect": "^24.9.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.17.0",
    "express-validator": "^6.2.0",
    "flash": "^1.1.0",
    "handlebars-helpers": "^0.10.0",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.2.2",
    "mocha-jenkins-reporter": "^0.4.2",
    "mongoose": "^5.7.8",
    "nodemailer": "^6.3.1",
    "nodemon": "^1.19.4",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-facebook-token": "^3.3.0",
    "passport-google-oauth20": "^2.0.0",
    "popper.js": "^1.16.0",
    "prom-client": "^12.0.0",
    "response-time": "^2.3.2",
    "sinon": "^7.5.0",
    "sinon-test": "^2.4.0",
    "swagger": "^0.7.5",
    "swagger-ui-express": "^4.1.2"
  }
}
